{
  "expo": {
    "entryPoint": "./src/App.tsx",
    "name": "liane",
    "slug": "liane",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSLocationUsageDescription": "This app collects your GPS location to anonymously register your car travels.",
        "NSLocationAlwaysUsageDescription": "This app collects your GPS location to anonymously register your car travels.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "bundleIdentifier": "tech.oxymore.liane"
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "package": "tech.oxymore.liane",
      "versionCode": 9,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      }
    },
    "description": ""
  }
}
